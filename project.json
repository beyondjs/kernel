{
  "scope": "beyond-js",
  "name": "kernel",
  "version": "0.1.5",
  "license": "MIT",
  "modules": {
    "path": "./"
  },
  "static": {
    "includes": "vendor"
  },
  "deployment": {
    "distributions": [
      {
