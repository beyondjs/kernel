{
  "scope": "beyond-js",
  "name": "kernel",
  "version": "0.1.0",
  "license": "MIT",
  "modules": {
    "path": "./"
  },
  "static": {
    "includes": "vendor"
  },
  "deployment": {
    "distributions": [
      {
        "name": "npm",
        "maps": "external",
        "ports": {
          "bundles": 9101
        },
        "npm": {
          "platforms": {
            "web": "web",
            "node": "node"
          }
        }
      },
      {
        "name": "web",
        "platform": "web",
        "ports": {
          "bundles": 9102
        },
        "ts": {
          "compiler": "tsc"
        }
      },
      {
        "name": "backend-tsc",
        "platform": "backend",
        "ports": {
          "bundles": 9103
        },
        "ts": {
          "compiler": "tsc"
        }
      }
    ]
  },
  "libraries": {
    "imports": [
      "@beyond-js/backend",
      "@beyond-js/local"
    ]
  }
}